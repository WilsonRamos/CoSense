--//		sumForExpr		::=	"sum" sumProdMinMaxBody .
production	kNoisyIrNodeType_PsumForExpr:			followset = {
										followset(kNoisyIrNodeType_PpredArithExpr),
									}

--//		productForExpr		::=	"product" sumProdMinMaxBody .
production	kNoisyIrNodeType_PproductForExpr:		followset = {
										followset(kNoisyIrNodeType_PpredArithExpr),
									}

--//		minForExpr		::=	"min" sumProdMinMaxBody .
production	kNoisyIrNodeType_PminForExpr:			followset = {
										followset(kNoisyIrNodeType_PpredArithExpr),
									}

--//		maxForExpr		::=	"max" sumProdMinMaxBody .
production	kNoisyIrNodeType_PmaxForExpr:			followset = {
										followset(kNoisyIrNodeType_PpredArithExpr),
									}

--//		sumProdMinMaxBody	::=	["for" varIntro ["from" predArithExpr "to" predArithExpr]] ["with" predExpr] "of" predArithExpr .
production	kNoisyIrNodeType_PsumProdMinMaxBody:		followset = {
										followset(kNoisyIrNodeType_PsumForExpr),
										followset(kNoisyIrNodeType_PproductForExpr),
