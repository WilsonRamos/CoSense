--//		setCmpTerm		::=	setExpr setCmpOp setExpr .
production	kNoisyIrNodeType_PsetCmpTerm:			followset = {
										followset(kNoisyIrNodeType_PpredTerm),
									}

--//		setFactor		::=	constSetExpr ":" typeExpr | "{" "}" | "omega"
