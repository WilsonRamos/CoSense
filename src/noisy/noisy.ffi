production	kNoisyIrNodeType_PsetFactor:			followset = {
										firstset(kNoisyIrNodeType_PhighPrecedenceBoolSetOp),
										followset(kNoisyIrNodeType_PsetTerm),
									}

--//		setTerm			::=	setFactor {highPrecedenceBoolSetOp setFactor}
