# LEDs
LED_0603_MASS	    : constant = 2 * (10E-6) * kg;
LED_0603_NUM        : constant = 3;
# Resistors
RESISTOR_0402_MASS  : constant = 0.8 * (10E-6) * kg;
RESISTOR_0402_NUM   : constant = 28;
# Capacitors
CAPACITOR_0402_MASS : constant = 0.09 * (10E-6) * kg;
CAPACITOR_0402_NUM  : constant = 37;
# Inductors
INDUCTOR_0603_MASS  : constant = 4 * (10E-6) * kg;
INDUCTOR_0603_NUM   : constant = 1;
# Generics ICs
IC_GENERIC_MASS	    : constant = 33.4 * (10E-6) * kg;
IC_GENERIC_NUM      : constant = 21;
# Supercaps
SUPERCAP_MASS	    : constant = 60 * (10E-6) * kg;
SUPERCAP_NUM        : constant = 9;
# PCB
PCB_AREA            : constant = 3046.8 * (10E-6) * sq;


